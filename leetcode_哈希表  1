class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) 
	{
        unordered_map<int,int> hash;
		vector<int> ans;
		for(int i=0;i<nums.size();i++)
		{
            hash[nums[i]]=i;
		}
		for(int i=0;i<nums.size();i++)
		{
            if(hash.count(target-nums[i])&&i!=hash[target-nums[i]])
			{
                ans={i,hash[target-nums[i]]};
			    break;
			}
		} 
		return ans;
    }
};
